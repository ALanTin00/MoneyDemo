<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_f9f9fa"
    android:orientation="vertical">
     <TextView
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:textColor="@color/color_33"
         android:textSize="@dimen/sp_20"
         android:layout_marginTop="@dimen/dp_20"
         android:layout_marginLeft="@dimen/dp_15"
         android:fontFamily="@font/ping_fang_bold"
         android:text="@string/setting"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_A1ABB9"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_30"
        android:layout_marginLeft="@dimen/dp_16"
        android:fontFamily="@font/ping_fang_bold"
        android:text="@string/common_use"/>
    <RelativeLayout
        android:id="@+id/fragment_setting_my_account_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@color/white"
        android:layout_marginTop="@dimen/dp_8">
        <ImageView
            android:id="@+id/my_account_iv"
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_14"
            android:src="@mipmap/my_information_icon"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_13"
            android:textColor="@color/color_303545"
            android:layout_toRightOf="@+id/my_account_iv"
            android:fontFamily="@font/ping_fang_bold"
            android:text="@string/my_account"
            android:layout_marginLeft="@dimen/dp_9"/>
        <ImageView
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_13"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_17"
            android:src="@mipmap/more_arrow"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_setting_notification_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@color/white"
        android:visibility="gone"
        >
        <ImageView
            android:id="@+id/my_notification_message"
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_14"
            android:src="@mipmap/notification_icon"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_13"
            android:textColor="@color/color_303545"
            android:layout_toRightOf="@+id/my_notification_message"
            android:text="@string/notification_center"
            android:layout_marginLeft="@dimen/dp_9"/>
        <ImageView
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_13"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_17"
            android:src="@mipmap/more_arrow"/>
    </RelativeLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/color_A1ABB9"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_40"
        android:layout_marginLeft="@dimen/dp_16"
        android:fontFamily="@font/ping_fang_bold"
        android:text="@string/others"/>
    <RelativeLayout
        android:id="@+id/fragment_setting_feedback_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@color/white"
        android:layout_marginTop="@dimen/dp_10">
        <ImageView
            android:id="@+id/opinion_feedback"
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_14"
            android:src="@mipmap/opinion_feedback"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_13"
            android:textColor="@color/color_303545"
            android:layout_toRightOf="@+id/opinion_feedback"
            android:fontFamily="@font/ping_fang_bold"
            android:text="@string/opinion_feedback"
            android:layout_marginLeft="@dimen/dp_9"/>
        <ImageView
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_13"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_17"
            android:src="@mipmap/more_arrow"/>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/fragment_setting_about_us_rl"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:background="@color/white"
        >
        <ImageView
            android:id="@+id/about_us"
            android:layout_width="@dimen/dp_23"
            android:layout_height="@dimen/dp_23"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/dp_14"
            android:src="@mipmap/about_us_icon"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_13"
            android:textColor="@color/color_303545"
            android:layout_toRightOf="@+id/about_us"
            android:text="@string/about_us"
            android:fontFamily="@font/ping_fang_bold"
            android:layout_marginLeft="@dimen/dp_9"/>
        <ImageView
            android:layout_width="@dimen/dp_6"
            android:layout_height="@dimen/dp_13"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/dp_17"
            android:src="@mipmap/more_arrow"/>
    </RelativeLayout>
    
</LinearLayout>